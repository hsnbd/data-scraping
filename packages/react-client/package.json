{
  "name": "react-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.15.2",
    "@mui/material": "^5.15.2",
    "@mui/x-data-grid": "^6.18.6",
    "axios": "^0.21.1",
    "i18next": "^21.6.14",
    "i18next-browser-languagedetector": "^6.1.3",
    "i18next-http-backend": "^1.4.0",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.16.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "sass": "^1.49.11",
    "swr": "^2.2.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:coverage": "rimraf coverage && react-scripts test --coverage --watchAll=false && nyc report",
    "lint": "eslint ./ --ext .js,.ts,.tsx",
    "lint:fix": "eslint ./ --ext .js,.ts,.tsx --fix",
    "stylelint": "stylelint '**/*.scss'",
    "stylelint:fix": "stylelint '**/*.scss' --fix",
    "codebase:lint": "npm run lint && npm run stylelint",
    "codebase:fix": "npm run lint:fix && npm run stylelint:fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@nimblehq/eslint-config-nimble-react": "^1.3.0",
    "@nimblehq/stylelint-config-nimble": "^1.1.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^18.2.18",
    "danger": "^10.9.0",
    "danger-plugin-istanbul-coverage": "^1.6.2",
    "eslint": "^8.11.0",
    "prettier": "^3.1.1",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.6.0",
    "typescript": "^4.6.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "coverageReporters": [
      "json"
    ]
  },
  "nyc": {
    "exclude": [
      "src/reportWebVitals.ts"
    ],
    "excludeAfterRemap": true
  }
}
